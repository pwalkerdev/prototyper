{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Prototyper",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"env": {
				"IsDebugInstance": "true"
			},
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"preLaunchTask": "${defaultBuildTask}"
		},
		{
			"name": "Headless",
			"type": "dotnet",
			"request": "launch",
			"projectPath": "${workspaceFolder}\\.modules\\Headless\\Headless\\Headless.csproj",
			"launchConfigurationId": "TargetFramework=;Headless"
		},
		{
            "name": "Headless (Skip Build)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}${/}.modules${/}Headless${/}Headless${/}bin${/}Debug${/}net8.0${/}Headless.dll",
            "args": ["-m", "Debug", "-l", "CSharp", "-i", "stream", "-t", "asdf"],
            "cwd": "${workspaceFolder}${/}.modules${/}Headless${/}Headless",
            "console": "integratedTerminal",
            "stopAtEntry": false
        }
	]
}